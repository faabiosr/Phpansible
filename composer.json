{
    "name": "Phpansible/Phpansible",
    "description": "Ansible's playbooks generator for PHP projects",
    "homepage": "http://phpansible.com",
    "autoload": {
         "psr-0": {
            "Phpansible\\Phpansible": "src/"
         }
    },
    "require": {
        "php": ">=5.4.0",
        "respect/rest": "dev-master",
        "respect/config": "dev-master",
        "twig/twig": "1.15.*@dev"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "3.7.*"
    }
}
